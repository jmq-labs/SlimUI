<?xml version="1.0" encoding="UTF-8"?>
<!-- SlimUI config file -->
<settings>
	<config>
		<site_identity>SlimUI</site_identity>
		<site_name>Slim UI</site_name>					
		<wwwroot>http://www.slim-ui.com/demo/</wwwroot>
		<dc_1></dc_1>
		<dc_2></dc_2>
		<dbserver></dbserver>
		<dbname></dbname>
		<dbuser></dbuser>
		<dbpass></dbpass>
		<ldapauth>false</ldapauth>
		<ldap_dn></ldap_dn>
		<ad_server_address></ad_server_address>
		<auth_user></auth_user>
		<auth_pass></auth_pass>
		<base_dn></base_dn>		
		<tokenstr>mySecretPhrase</tokenstr>
		<lang>EN</lang>
		<theme>default</theme>
		<sys_theme>default</sys_theme>
		<img_dir>content/img/</img_dir>		
		<ldap_mail_dn></ldap_mail_dn>
		<favicon>favicon.ico</favicon>
