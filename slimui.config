		<login_left_image>startup-img.jpg</login_left_image>
		<more_info_link>system/recovery.html</more_info_link>
		<allow_filters>true</allow_filters>
		<filter_by>user_department</filter_by>
		<filter_type>include</filter_type>
		<elogs>true</elogs>
		<mobile_enabled>true</mobile_enabled>
		<maintenance_mode>true</maintenance_mode>
		<dev_account>admin</dev_account>
		<ajax_cache>false</ajax_cache>
	</config>
	<appsmenu_filters>
		<wb_calendar>Administrador</wb_calendar>
		<wb_planner>Administrador</wb_planner>
		<wb_logs>Administrador,Informatica</wb_logs>
		<wb_sopit>Administrador,OEC,RRHH</wb_sopit>
		<wb_netman>Administrador,Informatica</wb_netman>					
	</appsmenu_filters>
	<cookie>
		<data>userid</data>
		<data>user_displayname</data>		
	</cookie>
